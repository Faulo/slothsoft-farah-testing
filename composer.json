{
	"name" : "slothsoft/farah-testing",
	"description" : "Constraints and test suite stubs for Farah modules.",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.4",
		"slothsoft/core" : "^1.13.19",
		"slothsoft/farah" : "^1.21.28",
		"symfony/panther" : "^1.1.2",
		"dbrekelmans/bdi" : "^1.2.0",
		"phpunit/phpunit" : "~9.6.23"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\FarahTesting\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\FarahTesting\\" : "tests/"
		}
	},
	"keywords" : [
		"testing"
	],
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.0.x-dev"
		}
	}
}